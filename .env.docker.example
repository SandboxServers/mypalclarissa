# Clara Docker Environment Configuration
# Copy this to .env and fill in your values

# --------------------------
# LLM Provider
#   openrouter | nanogpt
# --------------------------
LLM_PROVIDER=nanogpt

# --------------------------
# User / Project
# --------------------------
USER_ID=demo-user
DEFAULT_PROJECT=Default Project

# --------------------------
# Backend URL (for browser access)
# Set to your server's external IP/domain
# --------------------------
NEXT_PUBLIC_BACKEND_URL=http://localhost:8000

# --------------------------
# OpenRouter API
# --------------------------
OPENROUTER_API_KEY=sk-or-...
OPENROUTER_MODEL=moonshotai/kimi-k2-0905
OPENROUTER_SITE=http://localhost:3000
OPENROUTER_TITLE=Clara Assistant

# --------------------------
# NanoGPT API
# --------------------------
NANOGPT_API_KEY=sk-nano-...
NANOGPT_MODEL=moonshotai/Kimi-K2-Instruct-0905
NANOGPT_MEM0_MODEL=openai/gpt-oss-120b

# --------------------------
# OpenAI API (for embeddings)
# --------------------------
OPENAI_API_KEY=sk-proj-...
